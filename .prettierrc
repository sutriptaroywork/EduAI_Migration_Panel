{
  "printWidth": 100,
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "avoid",
  "useTabs": false,
  "endOfLine": "auto",
  "function": true,
  "overrides": [
    {
      "files": "*.js",
      "options": {
        "overrides": [
          {
            "files": "*.tsx",
            "options": { "function": true }
          }
        ]
      }
    }
  ]
}
